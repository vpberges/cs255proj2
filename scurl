#!/usr/bin/env python

import OpenSSL
import socket
import urlparse
import os
import sys


s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)                 

# s.bind(('',80))  ?

s.connect(("stackoverflow.com" , 80))
# s.sendall("GET /\r\n")
s.send('GET / HTTP/1.1\nHost: stackoverflow.com\n\n')
print s.recv(256)
s.close  